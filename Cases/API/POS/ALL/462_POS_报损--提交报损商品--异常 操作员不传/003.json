		"spec":{"TYPE":"str"},
		"product_name":{"TYPE":"str"},
		"category_id":{"TYPE":"int"},
		"img_url":{"TYPE":"str"},
		"purchase_price":{"TYPE":"str"},
		"stock":{"TYPE":"str"}
	},
	"key":{"stock":"data.specs.0.stock"}
}
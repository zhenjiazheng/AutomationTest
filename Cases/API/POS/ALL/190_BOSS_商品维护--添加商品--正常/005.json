{
	"input":{
			"method": "POST",
			"url":"{{pre.pos_url}}",
			"rest": "ja/v1/boss/commodity/add",
		    "headers": { "token": "{{key.token_boss}}","storeNo":"{{key.store_id}}"},
			"param":{
            	"status": 0,
				"barcode": "12345678909876",
				"brand": "test",
				"category": 510,
				"is_standard": false,
            	"desc_url": "http://www.baidu.com/",
				"image": "http://www.baidu.com/",
            	"manufacturer": "manufacturer",
				"moList": [],
				"name": "忍者",
				"price": "122",
				"quality_period": 0,
            	"shortcut": 1,
				"source": "面包",
            	"specs": [{
					"discount_price": "110",
					"member_price": "110",
					"purchase_price": "100",
					"unit": "100",
					"productNumber": 1000,
					"stock": "100",
					"sale_price": "120",
					"id": 2}],
            	"sale_net": false,
				"tag": [],
				"user_id": "{{key.store_id}}",
				"weighable": false
        }
	      },
    "output":{
		"statusCode":{"EQ":0},
		"msg":{"EQ":"成功"},
		"data":{"TYPE":"dict"},
		"status":{"TYPE":"int","eq":0},


		"name":{"TYPE":"str","eq":"忍者"},
		"brand":{"TYPE":"str","eq":"test"},
		"barcode":{"TYPE":"str","EQ":"12345678909876"},
		"specs":{"TYPE":"dict"},
		"price":{"TYPE":"str","eq":"122.00"},
		"source":{"TYPE":"str","eq":"面包"},
		"quality_period":{"TYPE":"int"},
		"unit":{"TYPE":"str","eq":"100"},
		"category_display":{"TYPE":"str"},
		"image":{"TYPE":"str","eq":"http://www.baidu.com/"},
		"manufacturer":{"TYPE":"str"},
		"sale_price":{"TYPE":"str","eq":"120.00"},
		"is_standard":{"eq":false},
		"purchase_price":{"eq":"100.00"},
		"member_price":{"eq":"110.00"},
		"discount_price":{"eq":"110.00"},
		"stock": {"type":"str"},
		"data.id":{"TYPE":"int"},
		"weighable":{"eq":false}},
  "key":{"pid1":"data.id"}
}
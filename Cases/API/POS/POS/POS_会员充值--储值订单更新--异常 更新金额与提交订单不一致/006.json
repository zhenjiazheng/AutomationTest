{
	"input":{
		    "author":"lancy",
			"method": "POST",
			"url":"{{pre.pos_url}}",
            "headers":{"token":"{{key.token_pos}}"},
			"rest": "/ja/pos/v1/recharge/{{key.orderId}}",
		    "param":{
				"cashier_id":{{key.store_id}},
				"cashier_name":"{{key.tel}}",
				"order_amount":"200.00",
				"pay_info":[{"pay_type":2}],
				"recharge":{
					"card_id":{{key.ruleId}},
					"member_id":{{key.member_id}},
					"member_name": "test_member",
					"member_tel":"13512345678"}
			}
	},
	"output":{
		"status_code":{"EQ":0}
}
}
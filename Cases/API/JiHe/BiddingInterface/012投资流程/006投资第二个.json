{
  "author": "lancy",
  "input": {
    "method": "POST",
    "url": "{{pre.jh_url}}",
    "rest": "ja/v1/invest/tender",
    "headers": {
      "client": "3",
      "Content-Type": "application/json",
      "token": "{{key.token}}"
    },
    "param": {
      "tenderId": "{{key.id2}}",
      "investMoney": 100,
      "payPassword": 111111
    }
  },
  "output": {
    "code": {
      "EQ": 0
    },
    "msg": {
      "TYPE": "str",
      "EQ": "投资成功"
    },
    "invest_money": {
      "EQ": 100
    },
    "invest_interest": {
      "TYPE": "float","LE":"100.0*{{key.rate2}}/100*{{key.borrow_time2}}/12"
    },
    "service_money": {
      "TYPE":"float",
      "LE": "-100.0*{{key.rate2}}/100*{{key.borrow_time2}}/12*0.0"
    }
    }

  }
